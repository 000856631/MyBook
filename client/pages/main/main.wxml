<view class="page">
    <view class="page__hd">
        <view class="page__title">图书扫描大师</view>
        <view class="page__desc">扫描图书背后的ISBN</view>
        
    </view>
    <view class="page__bd">
        <view class="weui-panel weui-panel_access">
            <view class="weui-panel__bd">
                <navigator url="../answer/answer" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
                    <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
                        <image class="weui-media-box__thumb" src="../../imageSource/admin.jpg" />
                    </view>
                    <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
                        <view class="weui-media-box__title">我的图书</view>
                        <view class="weui-media-box__desc">创建一个抢答空间</view>
                    </view>
                </navigator>
                <view  class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active" bindtap="runScanCode">
                    <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
                        <image class="weui-media-box__thumb" src="../../imageSource/user.jpeg" />
                    </view>
                    <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
                        <view class="weui-media-box__title">扫一扫</view>
                        <view class="weui-media-box__desc">进入抢答空间</view>
                    </view>
                </view>
            </view>
        </view>
    </view>
</view>